{
  "name": "hexlet/code",
  "description": "Page Analyzer - a site that analyzes specified pages for SEO suitability",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Hexlet"
    }
  ],
  "require": {
    "php": "~8.3.0",
    "slim/slim": "4.11",
    "slim/psr7": "1.6",
    "slim/http": "1.3",
    "slim/php-view": "3.2",
    "slim/flash": "0.4.0",
    "php-di/php-di": "7.0",
    "vlucas/phpdotenv": "5.5",
    "illuminate/support": "10.0",
    "nesbot/carbon": "2.66",
    "guzzlehttp/guzzle": "7.9.3",
    "imangazaliev/didom": "2.0",
    "slim/twig-view": "3.3",
    "monolog/monolog": "3.9"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.7",
    "phpunit/phpunit": "10.5.46",
    "mockery/mockery": "1.6.12",
    "symfony/var-dumper": "6.4.21",
    "fakerphp/faker": "1.24.1"
  },
  "scripts": {
    "phpcs": "phpcs",
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "config": {
    "platform": {
      "php": "8.3.20"
    }
  }
}
