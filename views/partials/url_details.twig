{% extends "partials/responsive_table.twig" %}

{% block table_body %}
    <tr>
        <td>ID</td>
        <td>{{ url.id }}</td>
    </tr>
    <tr>
        <td>Имя</td>
        <td>{{ url.name }}</td>
    </tr>
    <tr>
        <td>Дата создания</td>
        <td>{{ url.created_at is defined ? formatDate(url.created_at) : '' }}</td>
    </tr>
{% endblock %}
